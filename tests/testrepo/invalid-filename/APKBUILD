pkgname="invalid-filename"
pkgver="1.0"
pkgrel=0
pkgdesc="Dummy test package that fails to build"
url="https://gitlab.alpinelinux.org/alpine/aports"
arch="noarch"
license="MIT"

prepare() {
	mkdir -p "$builddir"
}

build() {
	touch $'bad\nfile'
}

check() {
	true
}

package() {
	mkdir -p "$pkgdir"
	cp -R * "$pkgdir"/
}

